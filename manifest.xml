<?xml version="1.0" encoding="UTF-8"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:type="MailApp">
  <Id>890bd943-b0c9-4392-8302-d6f500a3325f</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>Xerox TemplateHub</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="Xerox TemplateHub"/>
  <Description DefaultValue="Generate and insert email templates in Outlook using pre-defined layouts."/>
  
  <!-- Optional but recommended -->
  <IconUrl DefaultValue="https://jordan26-dev.github.io/xerox-templatehub/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://jordan26-dev.github.io/xerox-templatehub/icon-64.png"/>
  <SupportUrl DefaultValue="https://jordan26-dev.github.io/xerox-templatehub/"/>

  <Hosts>
    <Host Name="Mailbox"/>
  </Hosts>

  <Requirements>
    <Sets DefaultMinVersion="1.3">
      <Set Name="Mailbox"/>
    </Sets>
  </Requirements>

  <FormSettings>
    <Form xsi:type="ItemCompose">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://jordan26-dev.github.io/xerox-templatehub/xerox-templatehub.html"/>
        <RequestedHeight>550</RequestedHeight>
      </DesktopSettings>
    </Form>
  </FormSettings>

  <Permissions>ReadWriteItem</Permissions>
</OfficeApp>
